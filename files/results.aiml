<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

<topic name="RESULTS">

    <category>
        <pattern>RESULTS2</pattern>
        <template>
            Here are results.<delay>2</delay>
            
            <think><set name="topic">READY</set></think>
            <srai>NO</srai>
        </template>
    </category>
    
   <category>
        <pattern>RESULTS</pattern>
        <template>
            
            <think>
                <set var="lenHollandCodes">6</set>
                <set var="matchingCareers"> </set>
            </think>

            <condition var="lenHollandCodes">
                <li value="0"></li>
                <li>
                    <think>
                        
                        <set var="hollandCode"><map name="num2hollandcodes"><get var="lenHollandCodes" /></map></set>
                        <set var="hollandCodeIsMatched"><srai>HOLLAND CODE MATCH <get var="hollandCode" /></srai></set>

                        <condition var="hollandCodeIsMatched">
                            <li value="TRUE">
                                
                                <condition var="matchingCareers">
                                    <li value=" "></li>
                                    <li><set var="matchingCareers"><get var="matchingCareers" />, </set></li>
                                </condition>

                                <set var="matchingCareers"><get var="matchingCareers" /><map name="num2careers"><map name="hollandcodes2careernums"><get var="hollandCode" /></map></map></set>
                            </li>
                        </condition>
 
                       <set var="lenHollandCodes"><map name="predecessor"><get var="lenHollandCodes" /></map></set>
                       
                    </think>

                    <loop />
                </li>
                
            </condition>         

            <condition var="matchingCareers">
                <li value=" ">
                    My, myâ€¦<break />
                    I have no idea how you could fit into a computer science career.<delay>2</delay>
                    Sorry.<delay>2</delay>
                </li>
                <li>
                    Here are some good choices for you careerwise:<split />
                    <get var="matchingCareers" />.<delay>2</delay>
                </li>
            </condition>
            
            <think><set name="currentQuestionIndex">1</set></think>

        </template>
    </category>
    
    <category>
        <pattern>HOLLAND CODE MATCH *</pattern>
        <template>

            <think>
                <set var="explodedHollandCodes"><explode><star /></explode></set>
                <set var="isReturnValueSet">FALSE</set>
            </think>
            
            <condition var="explodedHollandCodes">
                <li value=" "></li>
                <li>
                    <think>
                        <set var="currentCode"><first><get var="explodedHollandCodes" /></first></set>
                        <set var="maxValue"><srai>XMAX <get name="rCode" /> <get name="iCode" /> <get name="aCode" /> <get name="sCode" /> <get name="eCode" /> <get name="cCode" /></srai></set>
                        <set var="explodedHollandCodes"><rest><get var="explodedHollandCodes" /></rest> </set>
                        
                        <condition var="currentCode">
                            <li value="R">
                                <set var="currentValue"><get name="rCode" /></set>
                                <set name="rCode">0</set>
                            </li>
                            <li value="I">
                                <set var="currentValue"><get name="iCode" /></set>
                                <set name="iCode">0</set>
                            </li>
                            <li value="A">
                                <set var="currentValue"><get name="aCode" /></set>
                                <set name="aCode">0</set>
                            </li>
                            <li value="S">
                                <set var="currentValue"><get name="sCode" /></set>
                                <set name="sCode">0</set>
                            </li>
                            <li value="E">
                                <set var="currentValue"><get name="eCode" /></set>
                                <set name="eCode">0</set>
                            </li>
                            <li value="C">
                                <set var="currentValue"><get name="cCode" /></set>
                                <set name="cCode">0</set>
                            </li>
                        </condition>
                        
                        <set var="currentValueIsMax"><srai>XSUB <get var="maxValue" /> XS <get var="currentValue" /></srai></set>
                    </think>
                    
                    <condition var="currentValueIsMax">
                        <li value="0"></li>
                        <li>
                            FALSE
                            
                            <think>
                                <set var="explodedHollandCodes"> </set>
                                <set var="isReturnValueSet">TRUE</set>
                            </think>
                        </li>
                    </condition>
                    
                    <loop />
                </li>
            </condition>
            
            <condition var="isReturnValueSet">
                <li value="FALSE">TRUE</li>
            </condition>

        </template>
    </category>
    
</topic>

</aiml>